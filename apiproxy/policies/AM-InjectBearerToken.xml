<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="AM-InjectBearerToken">
    <Set>
        <Headers>
            <Header name="authorization">Bearer {accesstoken.workday.access_token}</Header>
        </Headers>
    </Set>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>